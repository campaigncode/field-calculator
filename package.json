{
	"name": "field-calculator",
	"version": "1.0.0",
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"start": "expo start",
		"start-dev": "expo start --tunnel",
		"start-dev-clean": "expo start --clear --tunnel",
		"prebuild-ios": "rm -rf ./ios && expo prebuild --platform ios",
		"prebuild-android": "rm -rf ./android && expo prebuild --platform android",
		"build-ios": "eas build --platform ios --local",
		"build-android": "eas build --platform andriod --local",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web"
	},
	"dependencies": {
		"@gluestack-style/react": "^0.2.41",
		"@gluestack-ui/themed": "^0.1.36",
		"@react-navigation/bottom-tabs": "^6.5.8",
		"@react-navigation/native": "^6.1.7",
		"expo": "~49.0.10",
		"expo-status-bar": "~1.6.0",
		"react": "18.2.0",
		"react-native": "0.72.4",
		"react-native-keyboard-aware-scrollview": "^2.1.0",
		"react-native-safe-area-context": "^4.6.3",
		"react-native-screens": "^3.22.1",
		"react-native-svg": "13.9.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0"
	},
	"private": true
}
